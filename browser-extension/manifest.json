{
  "name": "url2app",
  "description": "Open any page / link / URL in external app using url2app:// protocol handler",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "storage", "contextMenus"],
  "icons": {
    "48": "images/logo-48.png",
    "96": "images/logo-96.png",
    "128": "images/logo-128.png"
  },
  "action": {},
  "options_ui": {
    "page": "options/index.html"
  },
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  }
}
